    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.33.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "handlebars": "^4.7.8",
    "handlebars-loader": "^1.7.3",
    "handlebars-webpack-plugin": "^2.2.1",
    "html-webpack-plugin": "^5.6.3",
