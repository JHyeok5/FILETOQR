  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --open --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "build:file-to-qr": "webpack --config webpack.config.js --entry ./assets/js/qr-generator/file-to-qr.js --output-filename assets/js/qr-generator/file-to-qr.bundle.js",
    "watch": "webpack --watch --config webpack.config.js --mode development",
    "lint": "eslint assets/js/**/*.js",
